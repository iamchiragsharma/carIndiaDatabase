					var countItem = 0;

					$.each(response, function (index, vehicleType) {
						ddlVehicleType.append($("<option></option>").val(vehicleType.VehicleTypeId).text(vehicleType.Title + " (" + vehicleType.Count + ")"));
						countItem++;
					});

					ddlBodyType.empty();

					$.ajax({
						type: "GET",
						url: "/includes/handlers/inventory.ashx",
						data: "t=dbt&vt=" + ddlVehicleType.val() + "&l=" + ddlLocation.val(),
						contentType: "application/json; charset=utf-8",
						dataType: "json",
						success: function (response) {
							var countItem = 0;
							ddlBodyType.append("<option value=''>Any Body Type</option>");
							$.each(response, function (index, bodyType) {
								ddlBodyType.append($("<option></option>").val(bodyType.BodyTypeId).text(bodyType.Title + " (" + bodyType.Count + ")"));
								countItem++;
							});

							if (countItem == 1) {
								$('#ddlBodyType option:eq(1)').attr('selected', 'selected');
							}
						}
					});

					ddlMake.empty();

					$.ajax({
						type: "GET",
						url: "/includes/handlers/inventory.ashx",
						data: "t=dmk&vt=" + ddlVehicleType.val() + "&l=" + ddlLocation.val(),
						dataType: "json",
						success: function (response) {
							var countItem = 0;
							var isToyotaInventoryEnable = 'True';
							var countNewStocks = 0;
							var countDemoStocks = 7;
							var vehicleType = getVehicleType();

							ddlMake.append("<option value='' selected>Any Make</option>");
							$.each(response, function (index, make) {
								var makeItemText = make.Title;
								if (isToyotaInventoryEnable == 'True') {
									if (vehicleType == "used" || (vehicleType == "new" && countNewStocks > 0) || (vehicleType == "demo" && countDemoStocks > 0)) {
										makeItemText += " (" + make.Count + ")";
									}
								}	
								else
								{
									makeItemText += " (" + make.Count + ")";
								}
								ddlMake.append($("<option></option>").val(make.MakeId).text(makeItemText));
								countItem++;
							});

							if (countItem == 1) {
								$('#ddlMake option:eq(1)').attr('selected', 'selected');
							}

							ddlModel.empty();
							$.ajax({
								type: "GET",
								url: "/includes/handlers/inventory.ashx",
								data: "t=dmd&sdmk=" + encodeURIComponent(ddlMake.val()) + "&vt=" + ddlVehicleType.val() + "&l=" + ddlLocation.val(),
								dataType: "json",
								success: function (response) {
									var countItem = 0;
									var isToyotaInventoryEnable = 'True';
									var vehicleType = getVehicleType();

									ddlModel.append("<option value='' selected>Any Model</option>");
									$.each(response, function (index, model) {
										ddlModel.append($("<option></option>").val(model.ModelId).text(model.Title + " (" + model.Count + ")"));
										countItem++;
									});

									if (countItem == 0 && isToyotaInventoryEnable == 'True' && vehicleType == "new") {						
										AppendToyotaNviModels();
									} 
									else {
										if (response.length == 0)
											ddlModel.prop("disabled", true);
										else
											ddlModel.prop("disabled", false);

										if (countItem == 1) {
											$('#ddlModel option:eq(1)').attr('selected', 'selected');
										}
									}
								}
							});
						}
					});
				}
			});
		});

		$('.btnSearchForVehiclesByOptions').click(function (e) {
			e.preventDefault();
		    $.ajax({
		        type: "GET",
		        url: '/webapi/api/1.0/search-box/get-product-category-singular',
		        data: "{}",
		        contentType: "application/json; charset=utf-8",
		        dataType: "json",
		        success: function (data, status) {
		        	var SingularCleaned = data;
		           
		            var link = "";
		            var param = "";
		            var vehicleType = "";


		            if (ddlVehicleType.val() != null && ddlVehicleType.val().length > 0) {
		                vehicleType = ddlVehicleType.val();
		            }
		            else {
		                vehicleType = "all";
		            }


					// begin Toyota New/Demo NVI redirects
		            var isToyotaInventoryEnable = 'True';		        	
		        	var toyotaNVIRoot = 'https://brianhiltontoyota.dealer.toyota.com.au/';
		        	var toyotaNVIUrlNew = 'https://brianhiltontoyota.dealer.toyota.com.au//new-vehicles/';
		        	var toyotaNVIUrlDemo = 'https://brianhiltontoyota.dealer.toyota.com.au//demonstrators/';		        	
					var countNewStocks = 0;
		        	var countDemoStocks = 7;
		        	if (isToyotaInventoryEnable == 'True') {
		        		if (vehicleType == "demo" && countDemoStocks == 0) {
		        			window.location = toyotaNVIUrlDemo;
		        			return false;
		        		} 
		        		if (vehicleType == "new" && countNewStocks == 0) {
		        			if (ddlModel.val() != '') {
		        				window.location = toyotaNVIUrlNew + ddlModel.val();
		        			} 
		        			else {
		        				window.location = toyotaNVIRoot;
		        			}
		        			return false;
		        		} 
					}
		        	// end Toyota New/Demo NVI redirects


		            var linkPrefix = "/" + vehicleType + "-" + SingularCleaned + "/";
		            var searchType = "All";

		            if (ddlMake.val() != null && ddlMake.val().length > 0) {
		                searchType = "MakeAndModel";
		            }

		            if (ddlBodyType.val() != null && ddlBodyType.val().length > 0) {
		                searchType = "BodyType";
		                linkPrefix = "/" + vehicleType + "-" + SingularCleaned + "/body-type/";
		            }

		            if (searchType == "All") {
		                linkPrefix = "/" + vehicleType + "-" + SingularCleaned + "/list";
		            }

		            if (searchType == "MakeAndModel") {
		                link = linkPrefix;

		                if (ddlMake.val() != null && ddlMake.val().length > 0) {
		                    link = link + ddlMake.val();
		                }

		                if (ddlModel.val() != null && ddlModel.val().length > 0) {
		                    link = link + "/" + ddlModel.val();
		                }

		                if (ddlBadge.val() != null && ddlBadge.val().length > 0) {
		                    link = link + "/" + ddlBadge.val();
		                }

		                link = link + "/";
		            }
		            else if (searchType == "BodyType") {
		                link = linkPrefix;

		                link = link + ddlBodyType.val() + "/";
		            }
		            else {
		                link = linkPrefix + "/";
		            }

		            var paramArray = [];

		            //Add filter
		            if (ddlPriceFrom.val() != null && ddlPriceFrom.val().length > 0) {
		                paramArray.push("pricefrom=" + ddlPriceFrom.val());
		            }

		            if (ddlPriceTo.val() != null && ddlPriceTo.val().length > 0) {
		                paramArray.push("priceto=" + ddlPriceTo.val());
		            }

		            if (ddlYearFrom.val() != null && ddlYearFrom.val().length > 0) {
		                paramArray.push("yearfrom=" + ddlYearFrom.val());
		            }

		            if (ddlYearTo.val() != null && ddlYearTo.val().length > 0) {
		                paramArray.push("yearto=" + ddlYearTo.val());
		            }

		            if (ddlLocation.val() != null && ddlLocation.val().length > 0) {
		                paramArray.push("location=" + ddlLocation.val());
		            }

		            if (paramArray.length > 0)
		                param = "?" + paramArray.join("&")

		            window.location = link.toLowerCase() + param.toLowerCase();
		            return false;

		        },
		        error: function (requeset, status, error) {
		            alert("error:"+error);
		        }
		    });

		    //var SingularCleaned = SearchScripts.GetProductCategory();

            
			
		});
	}
	
</script>
