<script>
    function save_rating(rating_value) {
            var ii=0;
            for(ii=0; ii<=5; ii++){									
                jQuery("#saverating_"+ii).removeClass("fa-star");
                jQuery("#saverating_"+ii).addClass("fa-star-o");									
            }
            for(ii=0; ii<=rating_value; ii++){									 
                    jQuery("#saverating_"+ii).removeClass("fa-star-o");
                    jQuery("#saverating_"+ii).removeClass("fa-star");
                    jQuery("#saverating_"+ii).addClass("fa-star");									
            }
            $('#rating').val(rating_value);
        }
</script>


