<script type="text/javascript">
 var subscribe = "0";
 jQuery(function($) {
        var currentMousePos = { x: -1, y: -1 };
        $(document).mousemove(function(event) {
            currentMousePos.x = event.screenX;
            currentMousePos.y = event.screenY;
            if (currentMousePos.x >= 1250 && currentMousePos.y <= 250 ) {                
                if(!$.cookie("user_subscribed") && subscribe == 0){
                  $("#footer_cookie").show();
                }
            }
        });
   });
   
 function acceptSubscribe(){
     name = $("#footer_cookie input[name = name]").val();
     phone = $("#footer_cookie input[name = phone1]").val();
     email = $("#footer_cookie input[name = email]").val();
     user_location= $("#footer_cookie input[name = user_location]").val();
     type = [];
        $.each($("input[name='type']:checked"), function(index){
              type[index]=$(this).val();
            });
       
     $.ajax({
        url: SITEURL + "/ajax/controller.php",
        type: 'post',
        data: {
            acceptSubscribe:1,
            name: name ,
            phone: phone ,
            email: email,
            user_location: user_location,
            type: type
        } ,
        dataType: 'json',
        success: function(json) {
           $.sticky(decodeURIComponent(json.message), { 
                autoclose: 9000,
                type: json.type,
                title: json.title
            });
            if(json.type =='success'){
                $('#footer_cookie').hide();
                $.cookie('user_subscribed', 1, { expires : 10 * 365 });
            }
        }
    });
 }
 function closesubscribe(){
    $('#footer_cookie').hide(); 
    subscribe = 1;
    $.ajax({
        url: SITEURL + "/ajax/controller.php",
        type: 'post',
        data: {closesubscribe:1} ,
        dataType: 'json',
        success: function(json) {
           
        }
    });
}
function closeWebRead(){
    $('#webread').hide();
}
</script> 

